<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Contatos.UserControls.PainelUC">
	<ContentView.Content>
        <Frame HasShadow="true" BackgroundColor="{DynamicResource corClara}" IsClippedToBounds="true">
            <Frame.OutlineColor>
                <OnPlatform x:TypeArguments="Color" Default="Transparent" Android="Black" iOS="Green" WinPhone="Gray"/>
            </Frame.OutlineColor>
            <Frame.Margin>
                <OnPlatform x:TypeArguments="Thickness" Default="10" Android="8" iOS="8" WinPhone="10"/>
            </Frame.Margin>
            <ContentView x:Name="cvConteudo" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand" />
        </Frame>
	</ContentView.Content>
</ContentView>
